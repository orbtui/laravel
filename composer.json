{
    "name": "orbtui/laravel",
    "description": "Fluid user interface, easy to use and integrated with Livewire.",
    "authors": [
        {
          "name": "Orbtall LLC",
          "email": "opensource@orbtall.com"
        }
      ],
    "keywords": [
        "ui",
        "user interface",
        "livewire",
        "alpine",
        "components"
    ],
    "homepage": "https://www.orbtui.com",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
      "files": [
        "src/Helpers/helpers.php"
      ],
      "psr-4": {
        "OrbtUI\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
          "providers": [
            "OrbtUI\\OrbtUIServiceProvider"
          ],
          "aliases": {
            "OrbtUI": "OrbtUI\\OrbtUI"
          }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "orbtui/laravel",
    "description": "Fluid user interface, easy to use and integrated with AlpineJS.",
    "type": "laravel-package",
    "authors": [
        {
          "name": "Orbtall LLC",
          "email": "opensource@orbtall.com"
        }
      ],
    "keywords": [
        "ui",
        "user interface",
        "alpine",
        "components"
    ],
    "homepage": "https://www.orbtui.com",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "livewire/livewire" : "^3.0"
    },
    "autoload": {
      "psr-4": {
        "OrbtUI\\": "src/"
      },
      "files": [
        "src/Helpers/helpers.php"
      ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
          "providers": [
            "OrbtUI\\OrbtUIServiceProvider"
          ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
